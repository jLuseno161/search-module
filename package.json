{
    "name": "search-module",
    "version": "0.0.0",
    "scripts": {
        "build:public": "ng build public --configuration production",
        "build:staff": "ng build staff --configuration production",
        "build:all": "npm run build:public && npm run build:staff",
        "deploy:public": "ng deploy public --base-href=/search/",
        "deploy:staff": "ng deploy staff --base-href=/search/staff/",
        "deploy:all": "npm run build:all && npx angular-cli-ghpages --dir=dist/public && npx angular-cli-ghpages --dir=dist/staff",
        "deploy": "npm run deploy:all"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^17.3.12",
        "@angular/cdk": "^17.3.10",
        "@angular/common": "^17.3.0",
        "@angular/compiler": "^17.3.0",
        "@angular/core": "^17.3.0",
        "@angular/forms": "^17.3.12",
        "@angular/material": "^17.3.10",
        "@angular/platform-browser": "^17.3.0",
        "@angular/platform-browser-dynamic": "^17.3.0",
        "@angular/router": "^17.3.12",
        "ngx-loading": "^17.0.0",
        "rxjs": "~7.8.0",
        "sweetalert2": "^11.26.3",
        "tslib": "^2.3.0",
        "zone.js": "~0.14.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^17.3.17",
        "@angular/cli": "^17.3.17",
        "@angular/compiler-cli": "^17.3.0",
        "@types/jasmine": "~5.1.0",
        "angular-cli-ghpages": "^2.0.3",
        "jasmine-core": "~5.1.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "typescript": "~5.4.2"
    }
}
